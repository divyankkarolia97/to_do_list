{

  "manifest_version": 2,
  "name": "to_do_list",
  "version": "1.0",

  "description": "A to do list addon",
  "homepage_url": "https://github.com/mdn/webextensions-examples/tree/master/beastify",
  "icons": {
    "48": "icon.png"
  },

  "permissions": [
    "activeTab"
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "to_do_list",
    "default_popup": "index.html"
  },

  "web_accessible_resources": [
   
  ]

}
